function out = nestd_cell_string_to_cell_string(c)
% out = nestd_cell_string_to_cell_string(c)
%  If c is a cell array containing cell arrays of strings
%  it converts it to a single cell array of string.

out = c;
if iscell(c) && all(cellfun(@iscell, c)) && all(cellfun(@(x) ischar(x{1}), c))
    out = cell(length(c), 1);
    for i=1:length(c)
        out{i} = c{i}{1};
    end;
elseif isa(c, 'containers.Map')
end

